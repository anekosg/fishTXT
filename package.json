{
  "name": "txt",
  "displayName": "txt",
  "description": "txt",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "my_extension_container",
          "title": "鱼说",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "my_extension_container": [
        {
          "id": "extension.myView",
          "name": "一款阅读工具"
        }
      ]
    },
    "commands": [
      ]
   
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.0.1",
    "typescript": "^4.4.3",
    "@vscode/test-electron": "^1.6.2"
  }
}